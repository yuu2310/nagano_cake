                        <% end %>
                      </li>
                    </ul>
                </div>
              </div>
            </div>  
             <% else %>
             <div class="navbar navbar-expand-lg navbar-dark bg-dark">
               <div class="container">
                 <div class="navbar-collapse collapse" id="navbarNavDropdown" style>
                   <ul class="navbar-nav ml-auto">
                     
                      <li>
                        <%= link_to about_path, class: "nav-link text-light" do %>
